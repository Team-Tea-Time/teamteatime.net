{
  "name": "ttt-backend",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "nodemon -w src --exec \"yarn run test && babel-node src --presets env\"",
    "build": "babel src -s -D -d dist --presets env",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "dependencies": {
    "express": "^4.15.2",
    "mongodb": "^2.2.26",
    "mongoose": "^4.9.5"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-preset-env": "^1.4.0",
    "eslint": "^3.19.0",
    "nodemon": "^1.11.0"
  }
}
