{
  "name": "ttt-backend",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "util": "babel-node src cli",
    "dev": "nodemon --legacy-watch -w src --exec \"yarn run test && babel-node src\"",
    "build": "babel src -s -D -d dist --presets env",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "dependencies": {
    "apicache": "^0.8.6",
    "aws-sdk": "^2.62.0",
    "bluebird": "^3.5.0",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "express-busboy": "^6.0.1",
    "express-validator": "^3.2.0",
    "github-api": "^3.0.0",
    "jsonwebtoken": "^7.4.0",
    "minimist": "^1.2.0",
    "mongodb": "^2.2.26",
    "mongoose": "^4.9.6",
    "mongoose-bcrypt": "^1.4.2",
    "mongoose-paginate": "^5.0.3",
    "mongoose-url-slugs": "^1.0.0",
    "morgan": "^1.8.1",
    "redis": "^2.7.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-preset-env": "^1.4.0",
    "eslint": "^3.19.0",
    "nodemon": "^1.11.0"
  }
}
